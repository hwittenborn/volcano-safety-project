{
  "compilerOptions": {
    "incremental": true,
    "target": "es2016",
    "module": "system",
    "outFile": "./index.js",
    "noEmit": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "allowUnreachableCode": false,
    "skipLibCheck": true
  }
}