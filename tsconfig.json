{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "jsx": "react",
    "module": "System",
    "moduleResolution": "node",
    "noEmit": false,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES6"
  }
}